{
    "cli": {
        "version": ">= 12.6.4",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "node": "20.18.0",
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            },
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "node": "20.18.0",
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            },
            "android": {
                "buildType": "apk"
            },
            "distribution": "internal"
        },
        "production": {
            "node": "20.18.0",
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            }
        }
    },
    "submit": {
        "production": {}
    }
}